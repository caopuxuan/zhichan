body{
    color: rgb(127, 127, 127);
    font-family: '微软雅黑', Tahoma, simsun, '宋体', arial;
	background-color: #fff;
}

/*审核记录*/
#mini-41{width: 90%}
#bg_layer{width:100%; height:100%; background:rgba(0,0,0,.2); position:fixed; top:0; bottom:0; left:0; right:0; z-index:100;display: none}
.shenhe{margin: 0 auto;width:100%; height:auto;}
.table-sh{padding-top:14px; border-top:1px solid #eee; max-height:424px; overflow-y:auto;}
.table-sh table{}
.table-sh table tr td,.table-sh table tr th{text-align:center;}
.table-sh table tr th{font-size:14px; color:#333; line-height:46px;}
.table-sh table .thd{border-bottom:2px solid #ddd;}
.table-sh table tr td{padding:15px 0; border-bottom:1px dashed #ddd; font-size:14px; color:#999;}
.tabli li{line-height:32px;list-style: none}
.tabli li img{display:inline-block; margin-left:4px;}
span.red{color:#fa0101; display:inline-block; margin-top:4px;}
.table-sh th span{ width:2px; height:20px; background:#ddd; font-size:0; float:left;margin-top: 14px;}
.table-sh th span.first{background:#007bd1;}
