body .yusuan-up .btn1{background:#73B009;}
body .yusuan-up .btn2{background:#F04545;}
body .yusuan-up .btn3{background:#007BD1;}
body .yusuan-up a:hover.mini-button{border:none;padding:1px;}
body .yusuan-up a:hover.btn1{background:#73B009;}
body .yusuan-up a:hover.btn2{background:#F04545;}
body .yusuan-up a:hover.btn3{background:#007BD1;}
.yusuan-up .mini-grid-headerCell{background:none; background:#E7EBEF; border-color:#ccc;}
.yusuan-up .mini-panel-border{border-color:#ccc;}

/*tab4--*/
.fouth-tabcont .mini-panel-border,.fivth-tabcont .mini-panel-border{border-color:#ccc;}
.fouth-tabcont .mini-grid-headerCell,.fivth-tabcont .mini-grid-headerCell{background:none; background: #E7EBEF;}
.fouth-tabcont,.fivth-tabcont{padding:25px 0;}

