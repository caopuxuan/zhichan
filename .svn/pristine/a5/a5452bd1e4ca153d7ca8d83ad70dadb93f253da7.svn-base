/*资产卡片详情--显示*/
#allSave{margin-left:10px;}


/*资产卡片-新增编辑文件上传样式*/
.report-file {display: block;position: relative; width:80px;overflow: hidden;color: #fff;cursor: pointer;text-align: center;margin-right:5px; border-radius: 5px;-moz-border-radius: 5px;-webkit-border-radius: 5px; left:50%; margin-left:-40px;background:#169BD5;}
.fielspan{cursor: pointer;display: block;line-height: 28px;}
.file-prew {
            position: absolute;
            top: 0;
            left:0;
            width: 80px;
            height: 28px;
            font-size: 100px; 
            opacity: 0; 
            filter: alpha(opacity=0);
            cursor: pointer;
    }
    
