/*资产卡片*/

.reim-main{padding:1%;}

.excision-check{margin-bottom: 15px;}
.excision-check .mini-toolbar{border:none; background: no-repeat;}
.search-tit{font-size:12px;}
.search-inform .mini-buttonedit-border,.search-inform .mini-textbox-border{border-radius: 3px;}

.excision-check .mini-button{background:none; color:#fff; border:none; border-radius: 3px;-moz-border-radius: 3px;-webkit-border-radius: 3px;}
 .excision-check  a:hover.mini-button{border:none;}
.excision-check .btn1{background:#007BD1;}
.excision-check .btn2{background:#73B009;}
.excision-check .btn3{background:#5D8C0C;}
.excision-check .btn4{background:#F04545;}
.excision-check .btn5{background:#007BD1;}
.excision-check .btn6{background:#D1A200;}
.excision-check .btn7{background:#9900CD;}
 .excision-check  a:hover.btn1{background:#007BD1;}
 .excision-check  a:hover.btn2{background:#73B009;}
 .excision-check  a:hover.btn3{background:#5D8C0C;}
 .excision-check  a:hover.btn4{background:#F04545;}
 .excision-check  a:hover.btn5{background:#007BD1;}
 .excision-check  a:hover.btn6{background:#D1A200;}
 .excision-check  a:hover.btn7{background:#9900CD;}
.search-inform{padding-bottom:20px;}
.search-inform .mini-checkbox{margin-left:10px;}

.report-table{}
.report-table .mini-grid-headerCell,.report-table .mini-grid-pager{background:none; background: #E7EBEF;}
.report-table .mini-panel-border{border:1px solid #ccc;}

.box{width: 100%;height: auto}
.box .min-left{width: 49%;height: auto;float: left}
.box .min-left .tit-head{width: 100%;height: 40px;border-bottom: 1px solid #ccc;text-align: center;line-height: 40px}
.box .min-right{width: 49%;height: auto;float: right}
.box .min-right .tit-head{width: 100%;height: 40px;border-bottom: 1px solid #ccc;text-align: center;line-height: 40px}


/*资产卡片新增*/
.add-tit{text-align:center;font-size: 18px;}
.report-tabs{margin-top:20px;}

/*保存关闭btns*/
.add-btns{}
.add-btns .mini-button{border:none; border-radius: 3px;-moz-border-radius: 3px;-webkit-border-radius: 3px; }
.add-btns .btn1{background:none; background:#007BD1;color: #fff;}
.add-btns .btn2{background:none; background:#e0e0e0;color: #333;}
 .add-btns a:hover.btn1{border:none; background:#007BD1;color: #fff;}
 .add-btns a:hover.btn2{border:none; background:#e0e0e0;color: #333;}

/*tab-tits*/
.report-tabs{}
.report-tabs .mini-tab{background:none; background:#e0e0e0; border-radius: 5px 5px 0 0;-moz-border-radius: 5px 5px 0 0;-webkit-border-radius: 5px 5px 0 0;border-color: #ccc;padding:6px 10px; font-size:14px; font-family: "微软雅黑";}
.report-tabs .mini-tab-active{background:#007BD1; color: #fff; border-color:#007BD1;}
.report-tabs .mini-tabs-bodys,.report-tabs .mini-tabs-space{border-color:#ccc;}

/*tab-1*/
.first-tabcont{padding: 25px 0;}
.first-tabcont .line1{margin-bottom:10px;}

.lineleft,.lineright{display:inline-block; width:49%; padding-left:10%; box-sizing: border-box;}
.boxline{display:inline-block; width:33%; box-sizing: border-box;}
.first-tabcont .line1 label,.first-tabcont .line1 input{display:inline-block; vertical-align:middle;}
.first-tabcont .line1 .mini-textbox,.first-tabcont .line1 .mini-buttonedit{width:60%!important; height: 32px;}
.first-tabcont .line1 #btnEdit1{width: 81%!important;}
.first-tabcont .line1 .mini-textarea{height: 62px;width: 81%!important;}
.first-tabcont .line1 .mini-textarea .mini-textbox-border{height: 60px;}
.first-tabcont .line1 label{width:80px;}
.first-tabcont .line1 .mini-textbox-border,.first-tabcont .line1 .mini-buttonedit-border{border-color:#ccc; border-radius: 5px;-moz-border-radius: 5px;-webkit-border-radius: 5px; height: 30px;}
.first-tabcont .line1 .mini-textbox-input,.first-tabcont .line1 .mini-buttonedit-input{height: 30px; line-height: 28px;}
.first-tabcont .line1 .mini-buttonedit-button{width:26px; height: 26px;}
.report-tabs .mini-required .mini-textbox-border,.report-tabs .mini-required .mini-buttonedit-border{ background:#fff3f3; border-color:#ffa8a8;}
.tb-rp{width: 30%;position: absolute;right: 3%;top: 12%}
.tb-rp .line1{width: 100%}
.tb-rp .line1 label{float: left;width: 20%}
.tb-rp .line1 .mini-popupedit{ float: right;width: 80%}
